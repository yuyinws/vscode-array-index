{
  "publisher": "yuyinws",
  "name": "vscode-array-index",
  "displayName": "Array Index",
  "type": "module",
  "version": "0.2.0",
  "private": true,
  "description": "Check array index in your editor.",
  "author": "yuyinws",
  "license": "MIT",
  "homepage": "https://github.com/yuyinws/vscode-array-index#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/yuyinws/vscode-array-index"
  },
  "bugs": {
    "url": "https://github.com/yuyinws/vscode-array-index/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.cjs",
  "icon": "res/icon.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.78.1"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-array-index.toggleArrayIndex",
        "title": "Toggle Array Index"
      }
    ],
    "configuration": [
      {
        "title": "Array Index",
        "properties": {
          "vscode-array-index.isShowInLoad": {
            "type": "boolean",
            "description": "whether show array index in load"
          }
        }
      }
    ]
  },
  "scripts": {
    "build": "rollup src/index.ts -c rollup.config.js",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish"
  },
  "dependencies": {
    "@humanwhocodes/momoa": "^3.0.0",
    "@vue/compiler-sfc": "^3.3.4",
    "pathe": "^1.1.2",
    "svelte": "^4.1.2",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.0",
    "@antfu/ni": "^0.21.5",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@types/node": "^18.17.3",
    "@types/vscode": "^1.78.1",
    "@vscode/vsce": "^2.20.1",
    "bumpp": "^9.1.1",
    "eslint": "^9.1.1",
    "esno": "^0.17.0",
    "fs-extra": "^11.2.0",
    "pnpm": "^8.6.12",
    "rimraf": "^5.0.1",
    "rollup": "^3.27.2",
    "rollup-plugin-esbuild": "^5.0.0",
    "vite": "^4.4.8",
    "vitest": "^0.34.1"
  }
}
